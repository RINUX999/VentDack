{
  "name": "ventdackbasic",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron:build": "tsc -p tsconfig.electron.json && mv dist-electron/main.js dist-electron/main.cjs",
    "electron": "electron dist-electron/main.cjs",
    "start": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && npm run electron:build && npm run electron\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^24.0.8",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/sqlite3": "^3.1.11",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "concurrently": "^9.2.0",
    "copyfiles": "^2.4.1",
    "electron": "^37.1.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0",
    "wait-on": "^8.0.3"
  }
}
